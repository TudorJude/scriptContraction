		var recArray = JSON.parse(recPool);
		for(var i = 0; i < recArray.length; i++)
		{
				  //set face to norbi
		  var setFace = server.UpdateUserData(
		  {
		  	PlayFabId: recArray[i].uId,
		  	Data : {"PicTexture" : oppTexture }
		  });
		}
}
