handlers.worthlessScript = function(args, context)
{
	//get norbi's picture 
	  var oppTexture = server.GetUserData(
	  {
	    PlayFabId:"A6ACA1A5111DA9C1",
	    Keys : ["PicTexture"]
	  }).Data.PicTexture.Value;


	  	//get all players in RecSubdivision1
		var td = server.GetTitleInternalData(
